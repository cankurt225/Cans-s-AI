type":"dataframe","variable_name":"df"}},"metadata":{},"execution_count":39}]},{"cell_type":"code","source":["import re\n","\n","# --- FONKSİYONLAR ---\n","\n","def convert_rooms(x):\n","    \"\"\"\n","    '3+1' -> 4\n","    'Studio (1+0)' -> 1\n","    '10 and more' -> 10\n","    \"\"\"\n","    if pd.isna(x):\n","        return 0\n","    s = str(x).lower()\n","\n","    if 'studio' in s:\n","        return 1\n","    if 'more than' in s:\n","        return 10\n","\n","    try:\n","        # '3+1' gibi ifadeleri topla\n","        parts = s.replace(' ', '').split('+')\n","        total = sum(float(p) for p in parts)\n","        return int(total)\n","    except:\n","        return 0\n","\n","def convert_floor_score(x):\n","    \"\"\"\n","    0: Giriş, Bodrum, Bahçe, Kot (Zemin ve altı)\n","    1: 1-3. Katlar (Alçak)\n","    2: 4-10. Katlar (Orta)\n","    3: 11+ Katlar ve Penthouse (Yüksek)\n","    \"\"\"\n","    if pd.isna(x):\n","        return 0\n","    s = str(x).lower()\n","\n","    # 0 Skoru: Zemin ve Altı\n","    low_keywords = ['garden', 'entrance', 'ground', 'basement', 'kot', 'villa', 'private', 'bahçe', 'giriş']\n","    if any(k in s for k in low_keywords):\n","        return 0\n","\n","    # 3 Skoru: En üst, Çatı\n","    if 'penthouse' in s or 'roof' in s:\n","        return 3\n","\n","    # Sayısal Katlar\n","    try:\n","        # Metin içindeki sayıyı bul (örn: \"5\" veya \"5. kat\")\n","        num = re.search(r'\\d+', s)\n","        if num:\n","            val = int(num.group())\n","            if val <= 3: return 1\n","            elif val <= 10: return 2\n","            else: return 3\n","    except:\n","        pass\n","    return 0\n","\n","def convert_heating_score(x):\n","    \"\"\"\n","    0: Soba, Yok, Bilinmiyor (Düşük Konfor)\n","    1: Doğalgaz, Kombi, Merkezi, Klima (Yüksek Konfor)\n","    \"\"\"\n","    if pd.isna(x):\n","        return 0\n","    s = str(x).lower()\n","    low_comfort = ['stove', 'soba', 'absent', 'yok', 'fireplace', 'solar', 'unknown']\n","\n","    # Eğer düşük konfor kelimelerinden biri geçiyorsa 0, yoksa 1\n","    if any(k in s for k in low_comfort):\n","        return 0\n","    return 1\n","\n","def convert_balcony(x):\n","    \"\"\"\n","    Available/Var -> 1\n","    Diğerleri -> 0\n","    \"\"\"\n","    if pd.isna(x):\n","        return 0\n","    if str(x).lower() in ['available', 'var', 'yes']:\n","        return 1\n","    return 0\n","\n","# --- UYGULAMA ---\n","\n","# Fonksiyonları ilgili sütunlara uyguluyoruz\n","df['Number of rooms'] = df['Number of rooms'].apply(convert_rooms)\n","df['Floor location'] = df['Floor location'].apply(convert_floor_score)\n","df['Heating'] = df['Heating'].apply(convert_heating_score)\n","df['Balcony'] = df['Balcony'].apply(convert_balcony)\n","\n","# Sonuçları Görelim\n","print(df[['Number of rooms', 'Floor location', 'Heating', 'Balcony']].head(10))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"oJz9FRuB_10Z","executionInfo":{"status":"ok","timestamp":1765651020881,"user_tz":-180,"elapsed":377,"user":{"displayName":"Can Ahmet Kurt","userId":"12470779629037628598"}},"outputId":"c30cf077-294b-4115-fc1c-3a9c08e139a3"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["   Number of rooms  Floor location  Heating  Balcony\n","0                4               1        1        1\n","1                2               0        1        0\n","2                4               2        1        0\n","3                4               0        1        0\n","4                4               2        1        1\n","5                3               1        1        1\n","6                4               1        1        1\n","7                4               2        1        1\n","8                3               2        1        1\n","9                3               1        1        1\n"]}]},{"cell_type":"code","source":["df.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":342